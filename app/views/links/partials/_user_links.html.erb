<ul class="list-group">
    <li class="list-group-item">
        <h5><%= link.title%></h5>
        <div class="row">
            <div class="col-md-6">
                <p><%= link_to short_link(link),
                original_url_path(link.vanity_string), target: "_blank" %></p>
                <%= link.clicks %> clicks
            </div>
            <div class="wrapper">
                <%= link_to "Manage", edit_link_path(link), class: "details-manage" %>
                <%= link_to "Details", link_path(link), class: "details-manage" %>
            </div>
        </div>
    </li>
</ul>
