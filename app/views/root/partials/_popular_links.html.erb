<div class="col-md-6">
  <% if @first_three_links %>
  <% @first_three_links.each do |toplink| %>
  <div class="panel panel-default">
    <div class="panel-body">
      <p><%= toplink.full_url %></p>
      <p><%= link_to short_link(toplink),
            original_url_path(toplink.vanity_string), target: "_blank" %></p>
      <span class="label label-as-badge label-primary"><%= toplink.clicks%>
        clicks</span>
    </div>
  </div>
  <% end %>
  <% else %>
  <%= render partial: "root/partials/no_links" %>
  <% end %>

</div>
<div class="col-md-6">
  <% if @next_two_links %>
  <% @next_two_links.each do |toplink| %>
  <div class="panel panel-default">
    <div class="panel-body">
      <p><%= toplink.full_url %></p>
      <p><%= link_to short_link(toplink),
            original_url_path(toplink.vanity_string), target: "_blank" %></p>
      <span class="label label-as-badge label-primary"><%= toplink.clicks%>
        clicks</span>
    </div>
  </div>
  <% end %>
  <% else %>
  <%= render partial: "root/partials/no_links" %>
  <% end %>
</div>
